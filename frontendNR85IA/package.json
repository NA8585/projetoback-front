    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "electron": "^36.2.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "vite": "^4.5.0",
    "wait-on": "^7.0.1"
  }
}
