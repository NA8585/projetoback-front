{
  "name": "telemetry-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "backend": "dotnet run --project \"../SuperBackendNR85IA/SuperBackendNR85IA.csproj\"",
    "dev:vite": "vite",
